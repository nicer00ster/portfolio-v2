{
  "version": 2,

  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/api/contact",
      "dest": "/api/index.js"
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "/api/index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "EMAIL": "@email-address",
    "PASSWORD": "@email-password",
    "CLIENT_ID": "@client-id",
    "CLIENT_SECRET": "@client-secret",
    "REFRESH_TOKEN": "@refresh-token"
  }
}